function getAfpadgCurrentDayId() {	var date = new Date();	var yy =(date.getFullYear()+"").substring(2);	var mm = (date.getMonth() + 1);	if (mm < 10) {		mm = "0" + mm	}	var dd = date.getDate();	if (dd < 10) {		dd = "0" + dd	}	var hh = date.getHours();	if (hh < 10) {		hh = "0" + hh	}	return parseInt(yy + "" + mm + "" + dd + "" + hh);}var pixel=document.getElementById("sid_adg_native_iftp");var accessFlag=null;var divId =null;if(pixel==null){	pixel=document.getElementById("afpftpPixel_365_1559503344305");var accessFlag=pixel.getAttribute('data-accessMode');var divId =pixel.getAttribute('data-divId');}var isDfpAppFlag =pixel.getAttribute('data-isDfpApp');var adgsrvrUrl = "";if(typeof isDfpAppFlag !='undefined' && isDfpAppFlag!=null && isDfpAppFlag==1){	adgsrvrUrl = "https://adgebra.co.in";}else if (window.location.protocol == "https:") {	adgsrvrUrl = "https://adgebra.co.in";} else {	adgsrvrUrl = "http://adgebra.co.in";}/**var pixel=document.getElementById("afpftpPixel_365_1559503344305");*/var pubId=pixel.getAttribute('data-pubid');var headerPixel=document.getElementById('adg_pixel_header_script');if(typeof headerPixel =='undefined' ||headerPixel==null){	var srcNode=document.createElement("script");	srcNode.src=adgsrvrUrl+"/afph/afphChild.js?p1="+pubId+"&p2=0&p3=0&rnd="+getAfpadgCurrentDayId();	srcNode.id="adg_pixel_header_script";	srcNode.type = 'text/javascript';		srcNode.async = true;	document.head.appendChild(srcNode);	}var siteId=pixel.getAttribute('data-siteid');var templateId=pixel.getAttribute('data-templateId');var domain=pixel.getAttribute('data-domain');var slotId=pixel.getAttribute('data-slotId');var dataGeoId=pixel.getAttribute('data-geoId');var isDfpApp=pixel.getAttribute('data-isDfpApp');var tpTemplateId=pixel.getAttribute('data-tpTemplateId');if(domain!="" && domain!=null ){ var  domainSplit=domain.split("amp-spike.html?domain="); if(domainSplit.length>1){	 domain= String(domainSplit[1].split("#")[0]);    }	 var domainSplitAmp=domain.split("/v/s/");	 if(domainSplitAmp.length>1){		domain=domainSplitAmp[1];	 } }if(typeof slotId=='undefined' || slotId==null || slotId=="")	{		slotId=0; /**slot Id 0 is for Unknown slot*/	}if(typeof templateId=='undefined' || templateId==null || templateId==""){	templateId=0; /**templateId Id 0 is for non IBN*/}if(typeof dataGeoId=='undefined' || dataGeoId==null || dataGeoId==""){/*For noral IBN without DFP*/	dataGeoId=0;}if(typeof isDfpApp=='undefined' || isDfpApp==null || isDfpApp==""){/*For noral IBN without DFP*/ isDfpApp=0;		}if(typeof tpTemplateId=='undefined' || tpTemplateId==null || tpTemplateId==""){	tpTemplateId=0; /**tpTemplateId Id 0 is for non IBN*/}if(siteId==1382){	/**this check for hindu,pixel given to hindu in that templateId not pass 	 * 	 */	templateId=1;	accessFlag="0";	divId= "adg_native_iftp" ;	isDfpApp=0;}var isMobileDevice = {		 		Android: function() {		 			return navigator.userAgent.match(/Android/i);		 		},		 		BlackBerry: function() {		 			return navigator.userAgent.match(/BlackBerry/i);		 		},		 		iOS: function() {		 			return navigator.userAgent.match(/iPhone|iPad|iPod/i);		 		},		 		Opera: function() {		 			return navigator.userAgent.match(/Opera Mini/i);		 		},                                Uc: function() {		 			return navigator.userAgent.match(/UCWEB|UCBrowser|OBUA UCBrowser/i);		 		},		 		Ucmini: function() {		 			return navigator.userAgent.match(/UC Mini/i);		 		},		 		Windows: function() {		 			return navigator.userAgent.match(/IEMobile/i);		 		},		 		any: function() {		 			return (isMobileDevice.Android() || isMobileDevice.BlackBerry() || isMobileDevice.iOS() || isMobileDevice.Opera() ||isMobileDevice.Uc());		 		}		 	};getAfpfTPTemplate(pubId,siteId,templateId,domain,slotId,accessFlag,divId,dataGeoId,isDfpApp,tpTemplateId);function getAfpfTPTemplate(pubId,siteId,templateId,domain,slotId,accessFlag,divId,dataGeoId,isDfpApp,tpTemplateId){  var xhttp = new XMLHttpRequest();  var keywordTargeting = getMetaTitleContentkeys();  var mongoKeyword="";  var checkDfp=0;  keywordTargeting =keywordTargeting.replace(/(\r\n|\n|\r)/gm,"");  keywordTargeting = keywordTargeting.replace(/  +/g, ' ');  var keywordTargetingArr=keywordTargeting.split("!");  if(keywordTargetingArr[1]==null||keywordTargetingArr[1]==""||keywordTargetingArr[1]==undefined){	  keywordTargetingArr[1]="";  }	        var adg_Metadetails={};  checkDfp=keywordTargetingArr[0].length;  if(keywordTargetingArr[0].length==0){	  	  /*  get keyword from mongo	   */	  							var xhr = new XMLHttpRequest();						xhr								.open(										"GET",										adgsrvrUrl+"/Adg_Analytics/GetMetaDetails?docurl="												+ domain												+ "&rand="												+ Math														.floor((Math																.random() * 30) + 1),										true);						xhr.setRequestHeader('Content-type',								'application/x-www-form-urlencoded');						xhr.onload = function() {							if (this.responseText == "NA"									|| this.responseText == "") {								adg_Metadetails = "";							} else {								adg_Metadetails = JSON										.parse(this.responseText);								var meta = adg_Metadetails['meta'];								if (meta != null || meta != ""										|| meta != undefined) {									keywordTargetingArr[0] = meta;									mongoKeyword = keywordTargetingArr[0]											+ "!" + keywordTargetingArr[1];																	}							}						};						xhr.send();	    		   }   setTimeout(function() {  xhttp.onreadystatechange = function() {	     if (this.readyState == 4 && this.status == 200) {      var template =this.responseText;      if(template!=undefined && template.length > 0){      var myObj = JSON.parse(template);      var htmlTemplate=myObj.template;           htmlTemplate=htmlTemplate.replace("#SLOT_ID_PLACE_HOLDER#",slotId);      htmlTemplate=htmlTemplate.replace("#TEMPLATE_ID_PLACE_HOLDER#",templateId);      htmlTemplate=htmlTemplate.replace("#GEOIP_PLACE_HOLDER#",dataGeoId);      htmlTemplate=htmlTemplate.replace("#ISDFPAPP_PLACE_HOLDER#",isDfpApp);      var tempateSize= myObj.templateSize;      tempateSize=tempateSize.split("X");      var ifrm=document.createElement("iframe");      if(myObj.displayFlag==1)  	{  		ifrm.style.display="block";  	}        else{        ifrm.style.display="none";         }      ifrm.id="ibniframe_"+divId;      document.getElementById(divId).appendChild(ifrm);           if(checkDfp>0){		  var keywordIBN=getMetaTitleContentkeys();    	   keywordIBN =keywordIBN.replace(/(\r\n|\n|\r)/gm,"");           keywordIBN = keywordIBN.replace(/  +/g, ' ');    	  htmlTemplate = htmlTemplate.replace("##KEYWORD_HOLDER##",    			  keywordIBN);      }else{		      	  htmlTemplate = htmlTemplate.replace("##KEYWORD_HOLDER##",    			  mongoKeyword);      }      ifrm.frameBorder = 0;     	  if(isMobileDevice.any())		 	{				            ifrm.width = "100%";							ifrm.height=tempateSize[1]+"px";							document.getElementById(divId).style.width="100%";							/*document.getElementById(divId).style.height=tempateSize[1]+"px";*/	                                                 		 	}            else                         {                                                      ifrm.width = tempateSize[0]+"px";                           ifrm.height= tempateSize[1]+"px";						/*document.getElementById(divId).style.width=tempateSize[0]+"px";                        document.getElementById(divId).style.height=tempateSize[1]+"px";*/	  		                                  }		          	ifrm = ifrm.contentWindow || ifrm.contentDocument.document || ifrm.contentDocument;	ifrm.document.open();	ifrm.document.write(htmlTemplate);	ifrm.document.close();    }       }  };  xhttp.open("GET", adgsrvrUrl+"/afpf/TPHtmlTemplate?p1="+pubId+"&p2="+siteId+"&p3="+slotId+"&p6="+templateId+"&p7="+accessFlag+"&p8="+tpTemplateId+"&p4="+domain, true);  xhttp.send();}, 500);}function getMetaTitleContentkeys() {	 var metaTags=document.getElementsByTagName("meta");	 var contentKeyword="";	 var contentTitle="";	  var content="";	 for (var i = 0; i < metaTags.length; i++) {	var metaName=metaTags[i].name;	var property = metaTags[i].getAttribute("property");	if(metaName.toLowerCase().indexOf("news_keywords".toLowerCase())!=-1 || metaName.toLowerCase().indexOf("News_Keywords".toLowerCase())!=-1) {		contentKeyword = metaTags[i].getAttribute("content");		break;	}	if(contentKeyword=="" ||contentKeyword==null ){		if(metaName.toLowerCase().indexOf("keyword".toLowerCase())!=-1 || metaName.toLowerCase().indexOf("Keyword".toLowerCase())!=-1)   	contentKeyword = metaTags[i].getAttribute("content");	}	if(property == "og:title")	contentTitle = metaTags[i].getAttribute("content");	}	if(contentTitle==null || contentTitle==""){	var titlesArr=document.getElementsByTagName("title");	var title='undefined';	if(titlesArr.length>0)	{title=document.getElementsByTagName("title")[0].value;	}	if(typeof(title)!=='undefined')	contentTitle=title;	 }	content=contentKeyword+"!"+contentTitle;	content = content.replace(/[?\/_|&\/_|#\/_]/g,' ');	content = content.replace(";",",");	content = content.substr(0, 1800);	 return content;	 	}